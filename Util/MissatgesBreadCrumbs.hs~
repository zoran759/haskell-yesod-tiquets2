bc_MailR = "user-get-mail" :: Text
bc_MailViewR = "user-view-mail" :: Text

bc_ElsMeusVolsR = "my-flights" :: Text
bc_TotsElsVolsR = "all-flights" :: Text

bc_CercarVolsR = "find-flights" :: Text
bc_CercarVolsResultR = "find-flights-result" :: Text

bc_CercarUsrsR = "admin-find-users" :: Text
bc_CercarUsrsResultR = "admin-find-users-result" :: Text

bc_UserInfoAdminVisitarR = "admin-view-user" :: Text
