module Util.MissatgesBreadCrumbs where

import Data.Text

renderAppBreadcrumbs :: -> GHandler sub App (GWidget sub app ())
renderAppBreadcrumbs = do
        (actual, bcAncestres) <- breadcrumbs
        return $(widgetFile "breadcrumbs")

bc_HomeR = "Home" :: Text
bc_UserInfoR = "profile" :: Text

bc_UserAddFlightR = "add-flight" :: Text
bc_UserEditFlightR = "edit-flight" :: Text
