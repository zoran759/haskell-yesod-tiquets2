$if null $ volsRecents
  <p>_{MsgEmpty}
$else
  <ul>
    $forall Entity volId vol <- volsRecents
       <li> #{UF.formataDay $ userVolDataAnada vol} #
            <span title="#{UF.nomDest $ userVolOrigen vol}">#{userVolOrigen vol}
            \ #
            <span title="#{UF.nomDest $ userVolDestí vol}">#{userVolDestí vol}


